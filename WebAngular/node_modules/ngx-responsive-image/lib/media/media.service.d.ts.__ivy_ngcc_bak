import { BreakpointObserver, BreakpointState } from '@angular/cdk/layout';
export declare class MediaService {
    private breakpointObserver;
    private breakpoints;
    private imageWidths;
    breakpoints$: import("rxjs").Observable<BreakpointState>;
    breakpointChange$: import("rxjs").Observable<[BreakpointState, BreakpointState]>;
    breakpointUp$: import("rxjs").Observable<string>;
    breakpointAndWidthUp$: import("rxjs").Observable<{
        width: any;
        breakpoint: string;
    }>;
    private maximumBreakpoint;
    constructor(breakpointObserver: BreakpointObserver, breakpoints: any, imageWidths: any);
}
