import { ElementRef, EventEmitter, OnChanges, OnDestroy, Renderer2, SimpleChanges } from '@angular/core';
import { MediaService } from '../media/media.service';
import { BreakpointChangeEvent } from './breakpoint-change-event';
export declare class ResponsiveImageDirective implements OnDestroy, OnChanges {
    private platformId;
    private element;
    private mediaService;
    private renderer2;
    imgSrc: string;
    manual: boolean;
    breakpointUp: EventEmitter<BreakpointChangeEvent>;
    private subscriptions;
    constructor(platformId: any, element: ElementRef<HTMLImageElement>, mediaService: MediaService, renderer2: Renderer2);
    ngOnChanges(simpleChange: SimpleChanges): void;
    ngOnDestroy(): void;
}
